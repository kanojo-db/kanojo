{
    "cSpell.words": [
        "ameblo",
        "birthdate",
        "fanart",
        "Fanza",
        "flagpack",
        "imdb",
        "inertiajs",
        "Kanojo",
        "kanojodb",
        "laravel",
        "luxon",
        "mgstage",
        "onlyfans",
        "Pinia",
        "Reactable",
        "Reacter",
        "reacterable",
        "Sluggable",
        "tightenco",
        "tiktok",
        "tmdb",
        "vuejs",
        "vuetify"
    ],
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "eslint.lintTask.enable": true,
    "eslint.lintTask.options": "--ext .js,.ts,.vue resources/js",
    "i18n-ally.displayLanguage": "en-US",
    "i18n-ally.enabledFrameworks": ["vue"],
    "i18n-ally.enabledParsers": ["json"],
    "i18n-ally.extract.autoDetect": false,
    "i18n-ally.ignoredLocales": ["en"],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.localesPaths": ["resources/js/locales"],
    "i18n-ally.pathMatcher": "{locale}.{ext}",
    "i18n-ally.sortKeys": true,
    "i18n-ally.sourceLanguage": "en-US",
    "laravel-pint.enable": true,
    "laravel-pint.preset": "laravel",
    "laravel-pint.runInLaravelSail": true,
    "stylelint.reportDescriptionlessDisables": true,
    "stylelint.reportInvalidScopeDisables": true,
    "stylelint.reportNeedlessDisables": true,
    "stylelint.validate": ["css", "less", "postcss", "vue", "scss"],
    "vitest.commandLine": "./vendor/bin/sail npm run test --",
    "vitest.enable": true,
    "sonarlint.testFilePattern": "{**/test/**,**/*test*,**/*Test*,**/*.spec.*}",
    "sonarlint.disableTelemetry": true,
    "sonarlint.connectedMode.project": {
        "projectKey": "kanojo-db_kanojo"
    }
}
