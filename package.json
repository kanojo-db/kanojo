{
    "private": true,
    "scripts": {
        "build": "vite build && vite build --ssr",
        "commit": "cz",
        "coverage": "vitest run --coverage",
        "dev": "vite",
        "lint": "eslint --ext .js,.ts,.vue resources/js",
        "prepare": "husky install",
        "test": "vitest",
        "type-check": "vue-tsc --noEmit"
    },
    "lint-staged": {
        "**/*.php": [
            "./vendor/bin/pint",
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "**/*.{css,scss}": [
            "prettier --write",
            "stylelint --fix"
        ],
        "**/*.vue": [
            "prettier --write",
            "stylelint --fix",
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "@inertiajs/vue3": "^1.0.11",
        "@sentry/tracing": "^7.69.0",
        "@sentry/vue": "^7.69.0",
        "@types/chroma-js": "^2.4.1",
        "@vueuse/core": "^10.4.1",
        "chart.js": "^4.4.0",
        "chroma-js": "^2.4.2",
        "copy-to-clipboard": "^3.3.3",
        "dotenv": "^16.3.1",
        "jsdom": "^22.1.0",
        "lodash-es": "^4.17.21",
        "luxon": "^3.4.3",
        "node-vibrant": "^3.2.1-alpha.1",
        "pinia": "^2.1.6",
        "puppeteer": "^21.1.1",
        "simple-icons": "^9.14.0",
        "swiper": "^10.2.0",
        "vite-plugin-vuetify": "^1.0.2",
        "vue-chartjs": "^5.2.0",
        "vue-i18n": "^9.4.1",
        "vuetify": "^3.3.16",
        "ziggy-js": "^1.6.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@commitlint/cz-commitlint": "^17.7.1",
        "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
        "@iconify-json/mdi": "^1.1.54",
        "@iconify/json": "^2.2.117",
        "@intlify/eslint-plugin-vue-i18n": "^3.0.0-next.3",
        "@intlify/unplugin-vue-i18n": "^1.2.0",
        "@intlify/vue-i18n-extensions": "^5.0.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@sentry/vite-plugin": "^2.7.1",
        "@shufo/prettier-plugin-blade": "^1.11.1",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/vue": "^7.0.0",
        "@types/is-number": "^7.0.3",
        "@types/jest-axe": "^3.5.6",
        "@types/lodash-es": "^4.17.9",
        "@types/luxon": "^3.3.2",
        "@types/ziggy-js": "^1.3.3",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "@vitejs/plugin-vue": "^4.3.4",
        "@vitest/coverage-istanbul": "^0.34.4",
        "@vue-macros/volar": "^0.14.3",
        "@vue/server-renderer": "^3.3.4",
        "@vue/test-utils": "^2.4.1",
        "@vue/vue3-jest": "^29.2.6",
        "@vueuse/head": "^1.3.1",
        "autoprefixer": "^10.4.15",
        "axios": "^1.5.0",
        "commitizen": "^4.3.0",
        "concurrently": "^8.2.1",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-jest": "^27.4.0",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "eslint-plugin-vue": "^9.17.0",
        "eslint-plugin-vuejs-accessibility": "^2.2.0",
        "husky": "^8.0.3",
        "is-number": "^7.0.0",
        "jest-axe": "^8.0.0",
        "laravel-vite-plugin": "^0.8.0",
        "lint-staged": "^14.0.1",
        "lodash": "^4.17.21",
        "postcss": "^8.4.29",
        "postcss-html": "^1.5.0",
        "prettier": "^3.0.3",
        "prettier-plugin-packagejson": "^2.4.5",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "sass": "1.66.1",
