{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "chelout/laravel-relationship-events": "^2.0",
        "cybercog/laravel-love": "9.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "halaxa/json-machine": "^1.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.6.9",
        "jackwh/laravel-new-relic": "^1.0",
        "jaybizzle/crawler-detect": "*",
        "jenssegers/agent": "^2.6",
        "knuckleswtf/scribe": "^4.18",
        "laravel/fortify": "^1.14",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.14",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.12",
        "meilisearch/meilisearch-php": "^1.0",
        "owen-it/laravel-auditing": "^13.0",
        "ramsey/uuid": "4.x-dev",
        "rennokki/laravel-eloquent-query-cache": "^3.4",
        "sentry/sentry-laravel": "^3.1",
        "spatie/enum": "^3.13",
        "spatie/laravel-csp": "^2.8",
        "spatie/laravel-data": "^3.0",
        "spatie/laravel-enum": "*",
        "spatie/laravel-medialibrary": "^10.6.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3.7",
        "spatie/laravel-translatable": "^6.1",
        "spatie/laravel-typescript-transformer": "^2.1",
        "spatie/typescript-transformer": "^2.1",
        "tightenco/ziggy": "^1.0",
        "werk365/etagconditionals": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.6",
        "laravel/pint": "^1.4",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper",
                "laravel/dusk",
                "pestphp/pest"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kanojo-db/etagconditionals"
        }
    ]
}