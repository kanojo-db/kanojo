{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "cybercog/laravel-love": "^8.9",
        "darkghosthunter/laraconfig": "master",
        "guzzlehttp/guzzle": "^7.2",
        "halaxa/json-machine": "^1.1",
        "inertiajs/inertia-laravel": "^0.6.3",
        "jenssegers/agent": "^2.6",
        "jordanmiguel/laravel-popular": "^1.0",
        "laravel/fortify": "^1.14",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "librenms/laravel-vue-i18n-generator": "^0.1.48",
        "owen-it/laravel-auditing": "^13.0",
        "protonemedia/laravel-cross-eloquent-search": "^3.0",
        "psalm/plugin-laravel": "^2.0",
        "sentry/sentry-laravel": "^3.1",
        "spatie/laravel-medialibrary": "^10.6.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3",
        "spatie/laravel-translatable": "^6.1",
        "teamtnt/laravel-scout-tntsearch-driver": "^12.1",
        "teamtnt/tntsearch": "^3.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "vimeo/psalm": "^4.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/ricventu/Laraconfig.git"
        }
   ]
}
